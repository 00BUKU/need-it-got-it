<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel='stylesheet' href='/stylesheets/style.css' />
  <title>NEEDIT, GOT IT</title>
</head>
<body>
  <h1>NEED IT GOT IT</h1>
  <div class="topnav">
    <a href="/stores/new">Stores</a>
    <a href="/logout">LOG OUT</a>
    <a href="/auth/google" class="login">LOG IN&nbsp;<img src="https://i.imgur.com/FHjYyi0.png" width=" 25px"></a>
  </div>
</div>
<form id="new-form" action="/stores" method="POST">
  <label>Store Name:</label>
  <input type="text" name="name">
  <input type="submit" value="Add Store"> 
</form>
  <table id="stores">
    <thead>
      <tr>
        <th>STORES</th>
      </tr>
    </thead>
    <tbody>
      <% stores.forEach(function(s) { %>
        <tr>
          <td><%= s.name %></td>
          <td><a href="/stores/<%= s._id %>">Items</a></td>
        </tr>
        <% }); %>
      </tbody>
    </table>
</body>
</html>